

<% flash.each do |name, msg| -%>
  <%= content_tag :div, msg, class: "#{name}" %>
<% end -%>
<h1> Add Category</h1>
<%= form_with model: @category, url: categories_path do |f| %>

    <div class="form-group">

      <%= f.label :name, "Name" %>
      <%= f.text_field :name %>
</div>
     <div>
      <%= f.label :icon, "Icon" %>
      <%= f.text_field :icon %>
    </div>
    <%= f.submit "Save"%>
        


<% end %>
